{
    "extends": "airbnb-base",
    "parserOptions": {
        "sourceType": "script",
        "ecmaVersion": 2018
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": "off",
        "no-param-reassign": "warn",
        "linebreak-style": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "eqeqeq": [2, "allow-null"]
    }
}